{
  "name": "wordpress-theme-koa",
  "description": "Koa wordpress theme, simple clean blog, a fully responsive lightweight theme. Compatible with all devices",
  "homepage": "https://github.com/jojoee/wordpress-theme-koa/",
  "authors": [
    "Nathachai Thongniran <inid3a@gmail.com>"
  ],
  "license": "GPL",
  "private": false,
  "dependencies": {
    "fastclick": "^1.0.6",
    "font-awesome": "^4.6.3",
    "slideout.js": "^0.1.12",
    "jeans-kit": "^1.1.2"
  },
  "overrides": {
    "fastclick": {
      "main": [
        "./lib/fastclick.js"
      ]
    },
    "font-awesome": {
      "main": [
        "./css/font-awesome.css",
        "./fonts/fontawesome-webfont.eot",
